{
    "name": "mohammadrezagohari/ghasedak-sms",
    "description": "This is a packge for using Ghasedak sms library very easier",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammadrezagohari",
            "email": "eng.mr.gohari@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MohammadrezaGohari\\GhasedakSms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohammadrezaGohari\\GhasedakSms\\GhasedakSmsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}